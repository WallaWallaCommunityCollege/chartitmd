{
    "name": "chartitmd/chartitmd",
    "license": "proprietary",
    "description": "Medical charting",
    "type": "project",
    "authors": [
        {
            "name": "Alec Aichele",
            "email": "aichelealec@gmail.com"
        },
        {
            "name": "Dragonrun1",
            "email": "Dragonrun1@gmail.com"
        },
        {
            "name": "James Wood",
            "email": "Jamesewood@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "vendor/prewk/result/helpers.php",
            "vendor/prewk/option/helpers.php"
        ],
        "psr-4": {
            "ChartItMD\\": [
                "bin/",
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Spec\\": "tests/specs/"
        },
        "psr-4": {
            "Tests\\Contexts\\": "tests/features/bootstrap/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2,<7.4",
        "ext-gmp": "*",
        "ext-PDO": ">=1.0.0",
        "ext-SPL": "*",
        "ext-apcu": "*",
        "ext-mbstring": "*",
        "ext-pdo_mysql": "*",
        "doctrine/orm": "^2.6",
        "dragonrun1/file_path_normalizer": "^2.1",
        "josegonzalez/dotenv": "^3.2",
        "monolog/monolog": "1.24.*",
        "php-di/slim-bridge": "^2.0",
        "potievdev/slim-rbac": "^1.0",
        "prewk/option": "^2.0",
        "slim/php-view": "^2.2",
        "slim/slim": "^3.12",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "behat/mink": "^1.7",
        "dragonrun1/git-change-log-creator": "^1.0",
        "guzzlehttp/guzzle": "~6.0",
        "leanphp/phpspec-code-coverage": ">=4.2 <5.0.0",
        "phpspec/nyan-formatters": ">=2.0.0 <3.0.0",
        "phpspec/phpspec": ">=4.0 <5.0.0",
        "phpunit/php-code-coverage": ">=5.0 <6.0.0",
        "roave/security-advisories": "dev-master"
    }
}
